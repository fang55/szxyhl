<resources>

    <!--登录输入文本-->
    <style name="login_et_text">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/thirty</item>
        <item name="android:textColor">@color/color_six_nine</item>
        <item name="android:drawablePadding">@dimen/w30</item>
        <item name="android:background">@null</item>
    </style>
    <!--登录下划线-->
    <style name="login_view_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/color_bule</item>
        <item name="android:alpha">0.5</item>
        <item name="android:layout_marginTop">@dimen/h30</item>
    </style>
    <style name="view_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/color_view_line</item>
        <item name="android:alpha">0.5</item>
        <item name="android:layout_marginTop">@dimen/h30</item>
    </style>
    <style name="login_btn">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/h80</item>
        <item name="android:background">@drawable/btn_blue_bg</item>
        <item name="android:textSize">@dimen/thirty_four</item>
        <item name="android:textColor">@color/color_white</item>
        <item name="android:gravity">center</item>
    </style>
