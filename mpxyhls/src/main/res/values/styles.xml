    <style name="iv_next">
        <item name="android:layout_width">@dimen/w16</item>
        <item name="android:layout_height">@dimen/h30</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:background">@mipmap/def_next</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <!--高为100 背景为白色-->
    <style name="ar_ll">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/h100</item>
        <item name="android:background">@color/color_white</item>
    </style>
    <!-- LinearLayout 背景色为白色，高为90-->
    <style name="set_ll">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/h90</item>
        <item name="android:paddingLeft">@dimen/w30</item>
        <item name="android:background">@color/color_white</item>
    </style>
    <!--高为70 背景为蓝色-->
    <style name="btn_sumbit">
        <item name="android:layout_width">@dimen/w500</item>
        <item name="android:layout_height">@dimen/h70</item>
        <item name="android:background">@drawable/btn_blue_bg</item>
        <item name="android:textSize">@dimen/twenty_eight</item>
        <item name="android:textColor">@color/color_white</item>
    </style>
    <style name="dialog" parent="@android:style/Theme.Dialog">
        <!-- 去黑边 -->
        <item name="android:windowFrame">@null</item>
        <!-- 设置是否可滑动 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 设置是否透明 -->
        <item name="android:windowIsTranslucent">true</item>
        <!-- 无标题 -->
        <item name="android:windowNoTitle">true</item>
        <!--
        点击空白部分activity不消失
        <item name="android:windowCloseOnTouchOutside">false</item>

        -->
    </style>
    <style name="push_style">
        <item name="android:hint">@string/style_hint</item>
        <item name="android:inputType">number</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="push_alias">
        <item name="android:hint">@string/alias_hint</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="push_tag">
