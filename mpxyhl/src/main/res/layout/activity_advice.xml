<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include
        layout="@layout/include_top"></include>
    <LinearLayout
        style="@style/set_ll"
        android:layout_marginTop="@dimen/h20"
        android:orientation="vertical"
        android:paddingTop="@dimen/h30"
        android:paddingLeft="@dimen/w30"
        android:paddingRight="@dimen/w30"
        android:background="@color/color_white">
        <EditText
            android:id="@+id/et_phone"
            style="@style/text_sixnine"
            android:background="@null"
            android:hint="@string/et_writephone"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/h100"
        android:layout_marginTop="@dimen/h20"
        android:background="@color/color_white"
        android:paddingLeft="@dimen/w30"
        android:paddingTop="@dimen/h10"
        android:orientation="vertical">
        <TextView
            android:id="@+id/et_question"
            style="@style/text_sixnine"
            android:background="@null"
            android:hint="@string/input_question"
            />
    </LinearLayout>

</LinearLayout>
