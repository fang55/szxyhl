<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingRight="@dimen/w30"
    android:paddingLeft="@dimen/w30"
    android:orientation="vertical">
  <RelativeLayout
      android:layout_width="match_parent"
      android:layout_height="@dimen/h60"
      >
      <TextView
          android:id="@+id/tv_incomeStates"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:textColor="@color/order_tv"
          android:textSize="@dimen/twenty_eight"
          android:layout_alignParentRight="true"
          android:text="已完成"/>
  </RelativeLayout>
   <RelativeLayout
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:layout_marginTop="@dimen/h20">
       <TextView
           android:id="@+id/tv_income1"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:textColor="@color/order_tv"
           android:textSize="@dimen/twenty_eight"
           android:text="已完成"/>
       <TextView
       android:id="@+id/tv_incomeNum"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:textColor="@color/order_tv"
       android:textSize="@dimen/twenty_eight"
       android:layout_toRightOf="@id/tv_income1"
       android:layout_marginLeft="@dimen/w30"
           android:layout_centerInParent="true"
       android:text="15单"/>
       <LinearLayout
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_alignParentRight="true"
           android:layout_centerVertical="true"
           android:orientation="horizontal">
       <TextView
           android:id="@+id/tv_incomeMoney"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:textColor="@color/order_tv"
           android:textSize="@dimen/twenty_eight"
           android:text="2200元"/>
       <TextView
           android:id="@+id/tv_incomeDetails"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:textColor="@color/order_tv"
           android:textSize="@dimen/twenty_eight"
           android:background="@drawable/shape_blue"
           android:layout_marginLeft="@dimen/w10"
           android:padding="@dimen/h10"
           android:text="查看详情"/>
       </LinearLayout>
   </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/h20">
        <TextView
            android:id="@+id/tv_income2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/order_tv"
            android:textSize="@dimen/twenty_eight"
            android:text="已提成收益"/>
        <TextView
            android:id="@+id/tv_alreadyNum"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/order_tv"
            android:textSize="@dimen/twenty_eight"
            android:layout_toRightOf="@id/tv_income2"
            android:layout_marginLeft="@dimen/w30"
            android:layout_centerInParent="true"
            android:text="15单"/>
        <TextView
            android:id="@+id/tv_alreadyMoney"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/order_tv"
            android:textSize="@dimen/twenty_eight"
            android:layout_alignParentRight="true"
            android:text="2200元"/>

    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/h20">
        <TextView
            android:id="@+id/tv_income3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/order_tv"
            android:textSize="@dimen/twenty_eight"
            android:text="待提成收益"/>
        <TextView
            android:id="@+id/tv_waitNum"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/order_tv"
            android:textSize="@dimen/twenty_eight"
            android:layout_toRightOf="@id/tv_income3"
            android:layout_marginLeft="@dimen/w30"
            android:layout_centerInParent="true"
            android:text="15单"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:orientation="horizontal">
        <TextView
            android:id="@+id/tv_waitMoney"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/order_tv"
            android:textSize="@dimen/twenty_eight"
            android:text="2200元"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/order_tv"
            android:textSize="@dimen/twenty_eight"
            android:background="@drawable/shape_blue"
            android:layout_marginLeft="@dimen/w10"
            android:padding="@dimen/h10"
            android:text="查看详情"/>
        </LinearLayout>
    </RelativeLayout>
</LinearLayout>
